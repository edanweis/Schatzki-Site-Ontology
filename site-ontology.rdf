    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#LinkedUnderstanding"/>
  </owl:Class>
  <owl:Class rdf:ID="SpatialRelation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Spatial relation</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#SocialRelation"/>
  </owl:Class>
  <owl:Class rdf:ID="Thing">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Thing</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Order">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Orders</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isArrangedBy"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isArrangedBy"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#SocialRelation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PrefigurativeRelation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Prefigurative relation</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#SocialRelation"/>
  </owl:Class>
  <owl:Class rdf:about="#Project">
    <rdfs:subClassOf rdf:resource="#Activity"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Project</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#isCarriedBy">
    <rdfs:domain rdf:resource="#Practice"/>
    <rdfs:range rdf:resource="#Entity"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="isComposedOf"/>
    </rdfs:subPropertyOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >performs practice</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isCarrierOf"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasRelation">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#isArrangedBy"/>
    </rdfs:subPropertyOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is related through</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:domain rdf:resource="#Entity"/>
    <rdfs:range rdf:resource="#SocialRelation"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isRelationTowards"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isParticipantIn">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is participant in</rdfs:label>
    <rdfs:domain rdf:resource="#Entity"/>
    <rdfs:range rdf:resource="#Activity"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#hasParticipant"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isComposedOf">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Activity"/>
          <owl:Class rdf:about="#Entity"/>
          <owl:Class rdf:about="#LinkedUnderstanding"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Practice"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >composes practice</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="composesPractice"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isEnactedBy">
    <rdfs:domain rdf:resource="#Practice"/>
    <rdfs:range rdf:resource="#Activity"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is enacted by</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="#isComposedOf"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isEnactmentOf"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#composesPractice">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Activity"/>
          <owl:Class rdf:about="#Entity"/>
          <owl:Class rdf:about="#LinkedUnderstanding"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >composes practice</rdfs:label>
    <owl:inverseOf rdf:resource="#isComposedOf"/>
    <rdfs:range rdf:resource="#Practice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasParticipant">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >has participant</rdfs:label>
    <owl:inverseOf rdf:resource="#isParticipantIn"/>
    <rdfs:domain rdf:resource="#Activity"/>
    <rdfs:range rdf:resource="#Entity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isEnactmentOf">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is enactedin</rdfs:label>
    <rdfs:domain rdf:resource="#Activity"/>
    <rdfs:subPropertyOf rdf:resource="#composesPractice"/>
    <rdfs:range rdf:resource="#Practice"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <owl:inverseOf rdf:resource="#isEnactedBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isRelationTowards">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Socially relates</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="arrangesEntities"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#SocialRelation"/>
    <rdfs:range rdf:resource="#Entity"/>
    <owl:inverseOf rdf:resource="#hasRelation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isCarrierOf">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:range rdf:resource="#Practice"/>
    <rdfs:domain rdf:resource="#Entity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is social relation for</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="#composesPractice"/>
    <owl:inverseOf rdf:resource="#isCarriedBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isOrganizedBy">
    <rdfs:range rdf:resource="#LinkedUnderstanding"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >social relation</rdfs:label>
    <rdfs:domain rdf:resource="#Practice"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Relations among entities are social relations to the extent that they help constitute social life. They form links among particular entities, (not types or hypostatized abstractions) which cover the full range of connections among entities of arrangements, not just links that join humans directly.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#isComposedOf"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isOrganizerOf"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#arrangesEntities">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >arranges entities</rdfs:label>
    <rdfs:range rdf:resource="#Entity"/>
    <rdfs:domain rdf:resource="#SocialRelation"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#isArrangedBy"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isArrangedBy">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >has social relation</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:range rdf:resource="#SocialRelation"/>
    <rdfs:domain rdf:resource="#Entity"/>
    <owl:inverseOf rdf:resource="#arrangesEntities"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isOrganizerOf">
    <rdfs:subPropertyOf rdf:resource="#composesPractice"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is organizer of</rdfs:label>
    <rdfs:domain rdf:resource="#LinkedUnderstanding"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:range rdf:resource="#Practice"/>
    <owl:inverseOf rdf:resource="#isOrganizedBy"/>
  </owl:ObjectProperty>
  <Artifact rdf:ID="surfboard">
    <hasRelation>
      <CausalRelation rdf:ID="float"/>
    </hasRelation>
  </Artifact>
  <Practice rdf:ID="surfing">
    <isOrganizedBy>
      <PracticalAbility rdf:ID="analysingWaves"/>
    </isOrganizedBy>
    <isEnactedBy>
      <Action rdf:ID="OceanWatching"/>
    </isEnactedBy>
    <isCarriedBy>
      <Human rdf:ID="EdanWeis"/>
    </isCarriedBy>
  </Practice>
</rdf:RDF>

