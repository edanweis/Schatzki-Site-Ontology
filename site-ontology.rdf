    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SocialRelation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Rules">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Rules</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#LinkedUnderstanding"/>
  </owl:Class>
  <owl:Class rdf:about="#Entity">
    <owl:disjointWith>
      <owl:Class rdf:about="#SocialRelation"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Activity"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isParticipantIn"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Entity</rdfs:label>
    <vs:term_status rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >testing</vs:term_status>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Entities are the components which make up social life.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="GeneralUnderstanding">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >General understanding</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#LinkedUnderstanding"/>
  </owl:Class>
  <owl:Class rdf:ID="Artifact">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Artifact</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Human">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Human</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Practice">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isCarriedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Activity"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isEnactedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Practice</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#LinkedUnderstanding"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isOrganizedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isOrganizedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isEnactedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Entity"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isCarriedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SocialRelation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Social relation</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="PracticalAbility">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Practical ability</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#LinkedUnderstanding"/>
  </owl:Class>
  <owl:Class rdf:ID="Action">
    <rdfs:subClassOf rdf:resource="#Task"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Action</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Organism">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Organism</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="TeleoaffectiveStructure">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
